<Window x:Class="StudentInfoSystem.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentInfoSystem"
        mc:Ignorable="d"
        Title="TestWindow" Height="450" Width="800">
    <Grid>
        <GroupBox Margin="0,0,0,115">
            <StackPanel Margin="0,0,0,276">
                <Label RenderTransformOrigin="0.503,0.55" Height="83">Wanna open in Test View????</Label>
            </StackPanel>
        </GroupBox>
        <GroupBox Margin="0,324,0,0">
            <StackPanel>
                <Button Height="35" Width="788" Click="Button_No">NO</Button>
                <Button Height="47" Click="Button_Yes">YES</Button>
            </StackPanel>
        </GroupBox>
    </Grid>
</Window>
